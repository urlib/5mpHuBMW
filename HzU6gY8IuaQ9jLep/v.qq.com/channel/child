<div class="site_subnav_placeholder"></div>
<script>
try{
var currentChannelNav = document.getElementsByClassName('site_channel') && document.getElementsByClassName('site_channel')[0].getElementsByClassName('current')
var currentChannelNavIcon = currentChannelNav.length && currentChannelNav[0].getAttribute('data-icon') || ''
if (currentChannelNav.length && currentChannelNavIcon) {
    currentChannelNav[0].innerHTML = '<img src="' + currentChannelNavIcon + '" alt="" class="channel_nav_icon">' + currentChannelNav[0].innerText
}

var unusualPic = document.getElementsByClassName('unusual_pic')
var unusualPicUrl = unusualPic.length && unusualPic[0].getAttribute('data-' + (window.channelInfos.parentEnName || window.channelInfos.channelId)) || ''
if (unusualPic.length && unusualPicUrl) {
   unusualPic[0].setAttribute('src',unusualPicUrl )
   unusualPic[0].setAttribute('class','unusual_pic' )
}

}catch(error){}
</script>

            
        
    

    
        
            
                
                
    
        
            
            
