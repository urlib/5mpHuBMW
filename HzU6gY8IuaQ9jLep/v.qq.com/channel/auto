    txv.common.initPage({
        boss: {
            app: '频道页',
            page: '汽车频道',
            isRecalc: true
        },
        lazyload: {
            getScreenShowHeight: (function (doc) {
                return function () {
                    var scrollTop = window.pageYOffset || doc_el.scrollTop;
                    return doc.clientHeight + scrollTop + 600;
                }
            })(doc_el)
        }
    });
